<?xml version="1.0" encoding="ISO-8859-1" ?>
<map-config
    max-bounds-left="321000"
    max-bounds-bottom="146000"
    max-bounds-right="361000"
    max-bounds-top="186000"
    max-zoom-levels="12"
    max-resolution="156.25"
    units="meters"
    projection="epsg:27700"
    initial-zoom="5"
    initial-x="341000"
    initial-y="166000"
    legend-control-named-maps=""
    disable-locator-marker="false"
    disable-map-picker="false"
    default-copyright ="North Somerset 100019726"
    disable-pan-control="true"
    disable-zoom-menu="true"  
    disable-slider-control="true" 
    gif-images-for-ie6 ="false">

<ZoomLevels>
    <ZoomLevel Value="2" Name="Entire District"></ZoomLevel>
    <ZoomLevel Value="5" Name="Locality"></ZoomLevel>
    <ZoomLevel Value="8" Name="Street"></ZoomLevel>
    <ZoomLevel Value="12" Name="Property"></ZoomLevel>
</ZoomLevels>

<summary-fields>
  <title-column value="Suffix"></title-column>
  <image-column value="Dataset_Char"></image-column>
  <link-column value="Policy_Ref"></link-column>
  <keyvalue-column value="Number"></keyvalue-column>
  <description-column value="Description"></description-column>
</summary-fields>

